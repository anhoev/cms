  "scripts": {
    "pretest": "npm install",
    "test": "./node_modules/.bin/karma start tests/karma.conf.js",
    "start": "node backend/use --config=./config/config.json --url=https://raw.githubusercontent.com/anhoev/cms_configs/master/digital_signage.json",
    "local": "node backend/use --url=https://raw.githubusercontent.com/anhoev/cms_configs/master/digital_signage.json",
    "start:dev": "node backend/use --env=development --url=https://raw.githubusercontent.com/anhoev/cms_configs/master/digital_signage.json",
    "safe": "node backend/use/index.js --mode=safemode",
    "digital-signage": "node backend/use/index.js --config=./config/digital-signage.json",
    "start:test": "export PATH_ENV=https://raw.githubusercontent.com/anhoev/cms_configs/master/digital_signage.json && node backend/use/index.js",
    "start:local": "export PATH_ENV=https://raw.githubusercontent.com/anhoev/cms_configs/master/test.json && node backend/use/index.js --mode=safemode",
    "pos": "node backend/use/index.js --config=./config/pos-config.json",
    "attendance": "node backend/use/index.js --config ./config/attendance-config.json",
    "cloudsignage": "node backend/use/index.js --config=./config/cloudsignage-config.json",
    "submodule-update": "git submodule foreach -q --recursive 'git checkout $(git config -f $toplevel/.gitmodules submodule.$name.branch || echo master) && git pull'",
    "submodule-ci-update": "git submodule update --init --recursive"
  },
